{
  "name": "vite-plugin-vitepress-auto-sidebar",
  "type": "module",
  "version": "1.5.0",
  "packageManager": "pnpm@7.25.1",
  "description": "",
  "author": "QC2168",
  "license": "MIT",
  "homepage": "https://github.com/QC2168/vite-plugin-vitepress-auto-sidebar#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QC2168/vite-plugin-vitepress-auto-sidebar.git"
  },
  "bugs": "https://github.com/QC2168/vite-plugin-vitepress-auto-sidebar/issues",
  "keywords": [
    "vitepress",
    "vite",
    "sidebar",
    "auto-sidebar"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint --fix --ext .ts src/ ",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.40.0",
    "eslint-config-standard-with-typescript": "^31.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "esno": "^0.16.3",
    "pnpm": "^7.12.0",
    "rimraf": "^3.0.2",
    "typescript": "^5.0.4",
    "unbuild": "^0.8.11",
    "vite": "^4.1.4",
    "vitepress": "1.0.0-alpha.49"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^5.59.2",
    "picocolors": "^1.0.0"
  },
  "peerDependencies": {
    "vite": "^4.0.4",
    "vitepress": "1.0.0-alpha.49"
  }
}
